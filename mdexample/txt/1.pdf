CREATE TABLE `points` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `lon` varchar(100) NOT NULL COMMENT '经度',
  `lat` varchar(100) NOT NULL COMMENT '纬度',
  `land_id` int(11) NOT NULL COMMENT '宗地id',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=7 DEFAULT CHARSET=latin1;
